+config_opts['legal_host_arches'] = ('armv5tel', 'armv6l', 'armv7l')
+config_opts['chroot_setup_cmd'] = 'groupinstall buildsys-build'
+config_opts['dist'] = 'fc18'  # only useful for --resultdir variable subst
